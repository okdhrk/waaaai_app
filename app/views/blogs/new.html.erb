<h1>ブログ投稿フォーム</h1>
<%= form_with model: @blog, local:true do |f| %>
  <h4>Title</h4>
  <%= f.text_field :title %>
  
  <h4>category</h4>
  <%= f.text_field :category %>
  
  <h4>本文</h4>
  <%= f.text_area :body %>
  
  <h4>Image</h4>
  <%= f.attachment_field :image %>
  <%= f.submit '投稿' %>
<% end %>  